Cell[TextData[{
 "Finally, it is more convenient if we convert the matrix to a piecewise \
function. First, we map ",
 StyleBox["StringRangeToNumberRange", "Code"],
 " over each of the first row, then we transpose the matrix and format it for \
",
 StyleBox["Piecewise", "Code"],
 " to handle."
}], "Text",
 CellChangeTimes->{{3.870257648151198*^9, 
  3.870257763375477*^9}},ExpressionUUID->"9981a8a4-059b-4d94-8548-\
dd188f8a2e98"],

Cell["Convert data matrix to piecewise function:", "CodeText",
 CellChangeTimes->{{3.8702577712483397`*^9, 
  3.8702577837865543`*^9}},ExpressionUUID->"aef3eafa-d168-4277-932b-\
57144858d246"],

Cell[BoxData[
 RowBox[{
  RowBox[{"ListToPiecewise", "[", 
   RowBox[{"data_List", ",", " ", "temperature_"}], "]"}], ":=", 
  RowBox[{"Piecewise", "[", "\n", "\t", 
   RowBox[{
    RowBox[{
     RowBox[{"{", 
      RowBox[{
       RowBox[{"Rest", "[", "#", "]"}], ",", " ", 
       RowBox[{"Between", "[", 
        RowBox[{"temperature", ",", " ", 
         RowBox[{"First", "[", "#", "]"}]}], "]"}]}], "}"}], "&"}], " ", "/@",
     " ", 
    RowBox[{"Transpose", "@", 
     RowBox[{"MapAt", "[", "\n", "\t\t", 
      RowBox[{"StringRangeToNumberRange", ",", "\n", "\t\t", 
       RowBox[{"data", " ", "*", " ", "units"}], ",", "\n", "\t\t", 
       RowBox[{"{", 
        RowBox[{"1", ",", " ", "All"}], "}"}]}], "\n", "\t", "]"}]}]}], "\n", 
   "]"}]}]], "Input",
 InitializationCell->True,
